                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "minStep": "",
                    "query": "kafka_cluster_partition_underreplicated{namespace=\"$namespace\", pod=~\"$app-.+$\"}",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
