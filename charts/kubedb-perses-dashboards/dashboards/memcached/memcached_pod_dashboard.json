                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "memcached_limit_bytes{pod=\"$pod\"}-memcached_current_bytes{pod=\"$pod\"}",
                    "seriesNameFormat": "free"
                  }
                }
              }
            }
          ]
        }
      },
      "5": {
