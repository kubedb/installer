{{- $shared := and (eq .Values.app.name "") (eq .Values.app.namespace "") -}}
{
  "kind": "Dashboard",
  "metadata": {
    "name": "PpBVXkwZk",
    "createdAt": "0001-01-01T00:00:00Z",
    "updatedAt": "0001-01-01T00:00:00Z",
    "version": 0,
    "project": "pp"
  },
  "spec": {
    "display": {
      "name": "KubeDB / Memcache / Pod"
    },
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "datasource",
            "hidden": false
          },
          "defaultValue": "Prometheus",
          "allowAllValue": false,
          "allowMultiple": false,
          "plugin": {
            "kind": "DatasourceVariable",
            "spec": {
              "datasourcePluginKind": "PrometheusDatasource"
            }
          },
          "name": "datasource"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "pod",
            "hidden": false
          },
          "defaultValue": "coreos-prom-memcd-0",
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "alphabetical-ci-asc",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "pod",
              "matchers": [
                "memcached_up"
              ]
            }
          },
          "name": "pod"
        }
      }
    ],
    "panels": {
      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "% Hit Ratio"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "sum(rate(memcached_commands_total{pod=\"$pod\",status=\"hit\"}[1m]))/sum(rate(memcached_commands_total{pod=\"$pod\"}[1m]))",
                    "seriesNameFormat": "hit ratio"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Commands"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "sum(rate(memcached_commands_total{pod=\"$pod\"}[1m]) * 60) by (command)",
                    "seriesNameFormat": ""
                  }
                }
              }
            }
          ]
        }
      },
      "2": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "% Get Ratio"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "sum(rate(memcached_commands_total{pod=\"$pod\",command=\"get\"}[1m]))/sum(rate(memcached_commands_total{pod=\"$pod\"}[1m]))",
                    "seriesNameFormat": "get ratio"
                  }
                }
              }
            }
          ]
        }
      },
      "3": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Connections"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {}
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "memcached_current_connections{pod=\"$pod\"}",
                    "seriesNameFormat": "used"
                  }
                }
              }
            },
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "memcached_max_connections{pod=\"$pod\"} - memcached_current_connections{pod=\"$pod\"}",
                    "seriesNameFormat": "free"
                  }
                }
              }
            }
          ]
        }
      },
      "4": {
